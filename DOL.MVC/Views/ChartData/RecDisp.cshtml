@model DotNet.Highcharts.Highcharts

@{
    ViewBag.Title = "Received and Dispatched";
}

@(Model)
<link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">

<div class="page-title">
    <h3>Items Received & Dispatched</h3>
    <div class="page-breadcrumb">
        <ol class="breadcrumb">
            Items are by percentages @ViewBag.ReportParam
        </ol>
    </div>
</div>

<div class="col-md-6">
    @using (Html.BeginForm("Search_RecDisp", "ChartData", FormMethod.Post))
    {
        <p>
            <table cellpadding="2" cellspacing="5">
                <tr>
                    <td> Sort Center:</td>
                    <td>
                        <select name="SortCenter" id="SortCenter">
                            <option value="DrakeWestEnd">Drake West End</option>
                            <option value="DrakeGillman">Drake Gillman</option>
                            <option value="DrakeLaverton">Drake Laverton</option>
                            <option value="DrakeStMarys">Drake St. Marys</option>
                        </select>
                    </td>
                    @*<td>Item</td>*@
                    @*<td>
                        <select name="URSCItem" id="URSCItem">
                            <option value="Pallete">Pallete</option>
                            <option value="Composite">Composite</option>
                            <option value="HollowProfile">Hollow Profile</option>
                            <option value="TopFrames">Top Frames</option>
                        </select>
                    </td>*@

                </tr>
                <tr>
                    <td></td>
                </tr>
                <tr>
                    <td>Received Date :</td>
                    <td> @Html.TextBox("RecDateFrom", "", new { style = "width:125px;height:25px", @class = "datepicker" })</td>
                    <td>To :</td>
                    <td> @Html.TextBox("RecDateTo", "", new { style = "width:125px;height:25px", @class = "datepicker" })</td>
                    <td><input type="submit" value="Search" /></td>
                </tr>
            </table>
        </p>


    }
</div>


